{
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": true,
                "regenerator": true
            }
        ],
        "add-module-exports",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
    ],
    "presets": [
        [
            "env",
            {
                "loose": true
            }
        ]
    ]
}